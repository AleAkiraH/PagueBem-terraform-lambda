environment             = "prod"
aws_region              = "us-east-1"
lambda_timeout          = 30
lambda_memory_size      = 512
log_retention_days      = 30

# Preencher ap√≥s deploy do DynamoDB (terraform output)
usuarios_table_name     = "Usuarios-prod"
usuarios_table_arn      = "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/Usuarios-prod"
qrcode_itens_table_name = "QrCodeItens-prod"
qrcode_itens_table_arn  = "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/QrCodeItens-prod"

dynamodb_table_arns = [
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/Usuarios-prod",
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/Usuarios-prod/index/*",
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/QrCodeItens-prod",
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/QrCodeItens-prod/index/*"
]
