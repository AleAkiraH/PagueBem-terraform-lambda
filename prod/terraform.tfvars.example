environment             = "prod"
aws_region              = "us-east-1"
lambda_timeout          = 30
lambda_memory_size      = 512
log_retention_days      = 30

# Preencher ap√≥s deploy do DynamoDB (terraform output)
usuarios_table_name     = "PagueBem-Usuarios-prod"
usuarios_table_arn      = "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Usuarios-prod"
produtos_table_name     = "PagueBem-Produtos-prod"
produtos_table_arn      = "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Produtos-prod"

dynamodb_table_arns = [
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Usuarios-prod",
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Usuarios-prod/index/*",
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Produtos-prod",
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Produtos-prod/index/*"
]
