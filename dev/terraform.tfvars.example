environment             = "dev"
aws_region              = "us-east-1"
lambda_timeout          = 30
lambda_memory_size      = 512
log_retention_days      = 14
jwt_secret              = "YOUR_JWT_SECRET_KEY_HERE"

# Preencher ap√≥s deploy do DynamoDB (terraform output)
usuarios_table_name     = "PagueBem-Usuarios-dev"
usuarios_table_arn      = "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Usuarios-dev"
produtos_table_name     = "PagueBem-Produtos-dev"
produtos_table_arn      = "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Produtos-dev"

dynamodb_table_arns = [
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Usuarios-dev",
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Usuarios-dev/index/*",
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Produtos-dev",
  "arn:aws:dynamodb:us-east-1:ACCOUNT_ID:table/PagueBem-Produtos-dev/index/*"
]
